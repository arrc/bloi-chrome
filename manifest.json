{
  "name": "4th-extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["bg.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "4th Browser action",
    "default_popup": "popup.html"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "http://*/*",
    "https://*/* "
  ]
}
